{
  "actions": ["yarn workspace infra deploy -c owner=${arg} -c region=${region}"],
  "argument": {
    "type": "string",
    "description": "Set environment to staging",
    "default": "dev"
  },
  "options": {
    "dev": {
      "description": "Run in dev mode",
      "shortcut": "d"
    },
    "region": {
      "shortcut": "r",
      "type": "string",
      "description": "Set region",
      "default": "us-east-2"
    }
  }
}
