{
  "name": "@mono-labs/tracker",
  "version": "0.1.276",
  "type": "commonjs",
  "description": "Code notation tracker for scanning, parsing, and managing structured comment markers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "tracker": "./bin/tracker.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "require": "./dist/dashboard/index.js",
      "default": "./dist/dashboard/index.js"
    }
  },
  "files": [
    "dist/",
    "dist-dashboard/",
    "bin/"
  ],
  "scripts": {
    "dev": "concurrently -n api,app -c blue,green \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "tsx src/dashboard/cli.ts",
    "dev:client": "yarn vite --config dashboard-app/vite.config.ts",
    "build": "tsc",
    "build:dashboard": "yarn vite build --config dashboard-app/vite.config.ts",
    "build:all": "tsc && yarn vite build --config dashboard-app/vite.config.ts",
    "test": "vitest run",
    "deploy": "tsc && yarn vite build --config dashboard-app/vite.config.ts && npm publish --access public --registry https://registry.npmjs.org/",
    "release:patch": "npm version patch -m \"chore: release %s\" && tsc && yarn vite build --config dashboard-app/vite.config.ts && npm publish --access public",
    "release:minor": "npm version minor -m \"chore: release %s\" && tsc && yarn vite build --config dashboard-app/vite.config.ts && npm publish --access public",
    "release:major": "npm version major -m \"chore: release %s\" && tsc && yarn vite build --config dashboard-app/vite.config.ts && npm publish --access public"
  },
  "license": "MIT",
  "dependencies": {
    "@mono-labs/shared": "0.1.276",
    "chokidar": "^4.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "fast-glob": "^3.3.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@tailwindcss/postcss": "^4.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^25.1.0",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "lucide-react": "^0.564.0",
    "postcss": "^8.5.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.2.0",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5.9.3",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  }
}
