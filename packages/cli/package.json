{
  "name": "@mono-labs/cli",
  "version": "0.1.270",
  "type": "commonjs",
  "description": "A CLI tool for building and deploying projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codymurphyjones/mono-labs-cli.git"
  },
  "bugs": {
    "url": "https://github.com/codymurphyjones/mono-labs-cli/issues"
  },
  "homepage": "https://github.com/codymurphyjones/mono-labs-cli#readme",
  "bin": {
    "mono": "./bin/mono.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node bin/mono.js",
    "dev": "node bin/mono.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "tsc && npm publish --access public --registry https://registry.npmjs.org/",
    "release:patch": "npm version patch -m \"chore: release %s\" && npm publish --access public",
    "release:minor": "npm version minor -m \"chore: release %s\" && npm publish --access public",
    "release:major": "npm version major -m \"chore: release %s\" && npm publish --access public"
  },
  "keywords": [
    "cli",
    "build",
    "deploy",
    "tool"
  ],
  "author": "Cody Jones",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@mono-labs/shared": "0.1.270",
    "@mono-labs/project": "0.1.270",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cross-spawn": "^7.0.6",
    "dotenv": "^17.2.0",
    "inquirer": "^12.8.2",
    "prettier": "^3.6.2",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "bin/",
    "lib/",
    "src/",
    "dist/",
    "README.md"
  ]
}
